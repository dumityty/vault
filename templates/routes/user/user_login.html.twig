{% extends 'base.html.twig' %}

{% block content %}
  <div class="row">
    <form id="credential-add" name="form-credential-add" method="post" action="" accept-charset="UTF-8" autocomplete="off">
    	<div class="form-group">
        <label for="edit-email" class="control-label">Email</label>
        <input type="textfield" id="edit-email" name="email" value="" size="15" maxlength="60" class="form-control required">
      </div>
      <div class="form-group">
	      <label for="edit-key" class="control-label">Password:</label>
	      <input type="password" id="edit-key" name="key" value="" size="15" maxlength="60" class="form-control required">
	    </div>
	    <div class="form-group clearfix" id="edit-actions">
				<input type="submit" id="edit-submit" name="op" value="Login" class="btn btn-default pull-right">
			</div>
   	</form>
{% endblock %}
